<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>EntityFramework.Patterns</id>
    <version>0.8</version>
    <title>EntityFramework.Patterns</title>
    <authors>Fabrice Michellonet</authors>
    <owners>Fabrice Michellonet</owners>
    <projectUrl>http://efpatterns.codeplex.com/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>EntityFramework.Patterns provides different patterns to be used with Entity Framework &gt;= 6.0
      Implemented patterns are : 
      - Generic Repository
      - Unit of Work
      - Repository Decorator
      - Auditable Entity
      - Archivable Entity
      - Cache
    </description>
    <summary>EntityFramework.Patterns provides different patterns easy and ready to used with Entity Framework &gt;= 6.0</summary>
    <dependencies>
      <dependency id="EntityFramework" version="6.0.1" />
    </dependencies>
  </metadata>
  <files>
    <file src="lib\net40\EntityFramework.Patterns.dll" target="lib\net40\EntityFramework.Patterns.dll" />
    <file src="lib\net40\Afterthought.dll" target="lib\net40\Afterthought.dll" />
    <file src="tools\Afterthought.Amender.exe" target="tools\Afterthought.Amender.exe" />
    <file src="tools\Afterthought.dll" target="tools\Afterthought.dll" />
    <file src="tools\Install.ps1" target="tools\Install.ps1" />
    <file src="tools\Uninstall.ps1" target="tools\Uninstall.ps1" />
  </files>
</package>